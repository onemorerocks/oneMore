{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "classes": true,
    "jsx": true
  },
  "plugins": [
    "classes",
    "filenames"
  ],
  "rules": {
    "arrow-body-style": 0,
    "brace-style": [
      2,
      "1tbs",
      {
        "allowSingleLine": false
      }
    ],
    "classes/name": [
      2,
      "class",
      "method"
    ],
    "comma-dangle": 0,
    "consistent-this": [
      2,
      "self"
    ],
    "filenames/filenames": [
      2,
      "^[A-Za-z][a-zA-Z0-9]*$",
      "match-exported-or-regex"
    ],
    "max-len": [2, 140],
    "no-else-return": [
      0
    ],
    "no-param-reassign": [2, {"props": false}],
    "no-underscore-dangle": ["error", { "allowAfterThis": true }],
    "no-unused-vars": [
      2,
      {
        "vars": "all",
        "args": "none"
      }
    ],
    "no-useless-constructor": 0,
    "operator-linebreak": [
      2,
      "after"
    ],
    "padded-blocks": 0,
    "prefer-template": 0,
    "quotes": [2, "single", "avoid-escape"],
    "react/jsx-closing-bracket-location": 0,
    "react/jsx-first-prop-new-line": 0,
    "react/jsx-indent-props": 0,
    "react/no-multi-comp": 0,
    "react/prefer-stateless-function": 0,
    "react/prop-types": [2, {"ignore": ["children", "history"]}]
  }
}
